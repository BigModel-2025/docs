{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Z.AI",
  "colors": {
    "primary": "#141618",
    "light": "#9fa0a0",
    "dark": "#141618"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "api": {
    "openapi": [
      "openapi.json"
    ],
    "playground": {
      "display": "interactive",
      "proxy": false
    },
    "examples": {
      "languages": [
        "bash",
        "python",
        "javascript",
        "java"
      ],
      "defaults": "required"
    }
  },
  "redirects": [
    {
      "source": "/guides/tools/web-search",
      "destination": "/"
    },
    {
      "source": "/api-reference/tools/web-search",
      "destination": "/"
    }
  ],
  "navigation": {
    "versions": [
      {
        "version": "English",
        "tabs": [
          {
            "tab": "Guides",
            "groups": [
              {
                "group": "Overview",
                "pages": [
                  "guides/overview/quick-start",
                  "guides/overview/pricing",
                  {
                    "group": "SDKs Guide",
                    "pages": [
                      "guides/develop/http/introduction",
                      "guides/develop/python/introduction",
                      "guides/develop/java/introduction",
                      "guides/develop/openai/python",
                      "guides/develop/langchain/introduction"
                    ]
                  }
                ]
              },
              {
                "group": "Language Models",
                "pages": [
                  "guides/llm/glm-4.5",
                  "guides/llm/glm-4-32b-0414-128k"
                ]
              },
              {
                "group": "Video Generation Models",
                "pages": [
                  "guides/video/cogvideox-3",
                  "guides/video/vidu-q1",
                  "guides/video/vidu2"
                ]
              },
              {
                "group": "Agents",
                "pages": [
                  "guides/agents/slide",
                  "guides/agents/translation",
                  "guides/agents/video-template"
                ]
              }
            ]
          },
          {
            "tab": "API Reference",
            "groups": [
              {
                "group": "Model API",
                "pages": [
                  "api-reference/llm/chat-completion"
                ]
              },
              {
                "group": "Video API",
                "pages": [
                  "api-reference/video/generate-video",
                  "api-reference/video/get-video-status"
                ]
              },
              {
                "group": "Agent API",
                "pages": [
                  "api-reference/agents/agent",
                  "api-reference/agents/file-upload",
                  "api-reference/agents/get-async-result",
                  "api-reference/agents/agent-conversation"
                ]
              }
            ]
          },
          {
            "tab": "Scenario Example",
            "groups": [
              {
                "group": "Develop Tools",
                "pages": [
                  "scenario-example/develop-tools/claude",
                  "scenario-example/develop-tools/kilo",
                  "scenario-example/develop-tools/roo",
                  "scenario-example/develop-tools/gemini",
                  "scenario-example/develop-tools/gork",
                  "scenario-example/develop-tools/cline",
                  "scenario-example/develop-tools/opencode"
                ]
              }
            ]
          },
          {
            "tab": "Terms and Policy",
            "pages": [
              "legal-agreement/privacy-policy",
              "legal-agreement/terms-of-use"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/dark.svg",
    "dark": "/logo/light.svg",
    "href": "https://z.ai"
  },
  "footer": {
    "socials": {
      "x": "https://x.com/Zai_org",
      "github": "https://github.com/zai-org",
      "discord": "https://discord.gg/QR7SARHRxK",
      "linkedin": "https://www.linkedin.com/company/zdotai/"
    }
  }
}
